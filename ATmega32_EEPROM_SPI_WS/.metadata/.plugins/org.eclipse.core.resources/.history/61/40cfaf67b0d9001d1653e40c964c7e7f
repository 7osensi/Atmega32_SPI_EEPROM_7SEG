#include "STD_TYPES.h"
#include "BIT_MATH.h"

#include "DIO_interface.h"
#include "DIO_private.h"
#include "DIO_config.h"

void MDIO_voidSetPinMode(u8 copy_u8PortId, u8 copy_u8PinId, u8 copy_u8PinMode)
{
	if(copy_u8PinMode == OUTPUT)
	{
		switch(copy_u8PortId)
		{
			case GPIOA:
							break;
			case GPIOB:
							break;
			case GPIOC:
							break;
			case GPIOD:
							break;
			default:		break;
		}
	}
	if(copy_u8PinMode == INPUT)
	{
		switch(copy_u8PortId)
		{
			case GPIOA:
							break;
			case GPIOB:
							break;
			case GPIOC:
							break;
			case GPIOD:
							break;
			default:		break;
		}
	}

}


